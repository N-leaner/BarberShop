<div>	
	<table class="table table-striped">
		<thead>
			<tr>			
			<th>Id</th>
			<th>User</th>
			<th>Phone</th>
			<th>DateTime</th>
			<th>Barber</th>
			</tr>
		</thead>
		<tbody>
			<%@result.each do |row|%>
				<tr>
					<td><%=row['id']%></td>
					<td><%=row['username']%></td>
					<td><%=row['phone']%></td>
					<td><%=row['datestamp']%></td>		
					<td><%=row['barber']%></td>		
				</tr>
			<%end%>			
		</tbody>	
	</table>
</div>