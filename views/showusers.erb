<div>	
	<table class="table table-bordered" style="text-align: center;">

		<tr class="success">
			<td>Id</td>
			<td>User</td>
			<td>Phone</td>
			<td>DateTime</td>
		</tr>

		<%@db.execute 'select * from Users order by Id desc' do |row|%>
			<tr>
				<td><%=row['id']%></td>
				<td><%=row['username']%></td>
				<td><%=row['phone']%></td>
				<td><%=row['datestamp']%></td>		
			</tr>
		<%end%>

	</table>
</div>